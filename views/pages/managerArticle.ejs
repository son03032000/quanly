<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/header') %>
</head>
    
<body>
    <%- include('../partials/topnav') %>
    <div>
        <ul>
            <% for (let article of data.Articles) { %>
                <li>
                    <a href="/Article/<%- article.ArticleID %>"><%- article.Title %> (Author: <%- article.Username %>, Topic: <%- article.TopicName %>) - <%- article.Status %></a>
                </li>
            <% } %>
        </ul>
    </div>
</body>

</html>